{
    "name": "next-mdx-static-blog-kit",
    "version": "1.0.0",
    "description": "A starter kit for creating a static website blog using Next.js and MDX",
    "scripts": {
        "dev": "NODE_ENV=development next",
        "build": "NODE_ENV=production next build",
        "build-dev": "NODE_ENV=development next build",
        "start": "NODE_ENV=production next start",
        "clean": "rm -rf .next ; npm ci ; NODE_ENV=development npm run dev",
        "now-build": "next build && next export -o dist",
        "test": "jest"
    },
    "keywords": [
        "next.js",
        "react.js",
        "blog",
        "mdx"
    ],
    "author": "Loren Stewart",
    "license": "ISC",
    "bin": {
        "blog": "cli.js"
    },
    "browserslist": [
        "last 2 version"
    ],
    "dependencies": {
        "@mdx-js/loader": "^1.5.2",
        "@zeit/next-mdx": "^1.2.0",
        "babel-plugin-preval": "^4.0.0",
        "chalk": "^3.0.0",
        "js-yaml": "^3.13.1",
        "js-yaml-loader": "^1.2.2",
        "next": "^9.3.2",
        "next-fonts": "^0.19.0",
        "next-redux-wrapper": "^6.0.0-rc.7",
        "next-seo": "^3.4.0",
        "react": "^16.12.0",
        "react-anchor-link-smooth-scroll": "^1.0.12",
        "react-dom": "^16.12.0",
        "react-icons": "^3.10.0",
        "react-redux": "^7.2.0",
        "react-syntax-highlighter": "^9.0.1",
        "redux": "^4.0.5",
        "remark-emoji": "^2.0.2",
        "remark-images": "^1.0.0",
        "scrollprogress": "^3.0.2",
        "use-dark-mode": "^2.3.1",
        "yargs": "^15.0.2"
    },
    "devDependencies": {
        "@babel/runtime-corejs2": "^7.9.2",
        "imagemin-gifsicle": "^6.0.1",
        "imagemin-mozjpeg": "^8.0.0",
        "imagemin-optipng": "^7.1.0",
        "imagemin-svgo": "^7.0.0",
        "jest": "^25.2.3",
        "next-optimized-images": "^2.5.4",
        "svg-sprite-loader": "^4.1.6",
        "webp-loader": "^0.6.0"
    }
}
